{
  "name": "papr-nest",
  "version": "1.0.0",
  "description": "MongoDB TypeScript-aware Models for NestJS",
  "main": "index.js",
  "scripts": {
    "test": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:watch": "jest --watch --config ./tests/jest-e2e.json --runInBand",
    "build": "rimraf dist && tsc",
    "prepublish": "npm run build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "preformat": "npm run lint",
    "format": "prettier --write \"**/*.ts\""
  },
  "author": "andersonjoseph",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "mongodb": "^4.6.0",
    "papr": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "dependencies": {},
  "devDependencies": {
    "rimraf": "^3.0.2",
    "fast-glob": "^3.2.11",
    "@nestjs/platform-express": "8.4.5",
    "@nestjs/testing": "^8.4.5",
    "@types/jest": "27.5.1",
    "@types/node": "^17.0.35",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.3",
    "typescript": "^4.7.2"
  }
}
